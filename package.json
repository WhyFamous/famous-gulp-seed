{
  "name": "famous-gulp-seed",
  "version": "0.9.0",
  "description": "seed project for a browserified version using Gulp of the Famous Engine",
  "browser": "src/index.js",
  "scripts": {
    "build": "gulp production",
    "build-bundle": "browserify src/index.js -g uglifyify | uglifyjs --screw-ie8 -m -c dead_code,sequences,conditionals,booleans,unused,if_return,join_vars,drop_debugger > public/js/bundle.js",
    "build-dev": "browserify src/index.js > public/js/bundle.js",
    "dev": "gulp dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WhyFamous/famous-gulp-seed.git"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "browser-sync": "^2.7.4",
    "browserify": "^10.1.3",
    "del": "^1.2.0",
    "gulp": "^3.8.11",
    "gulp-changed": "^1.2.1",
    "gulp-filesize": "0.0.6",
    "gulp-jshint": "^1.11.0",
    "gulp-minify-css": "^1.1.1",
    "gulp-notify": "^2.2.0",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.4",
    "jshint-stylish": "^2.0.0",
    "karma": "^0.12.32",
    "lodash": "^3.9.3",
    "merge-stream": "^0.1.7",
    "pretty-hrtime": "^1.0.0",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.0",
    "serve": "^1.4.0",
    "uglify-js": "^2.4.15",
    "uglifyify": "^3.0.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.1.0"
  },
  "author": "Tony Alves",
  "contributors": [
    {
      "name": "Tony Alves",
      "url": "https://github.com/talves/"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "babelify": "^6.0.1",
    "famous": "git://github.com/famous/engine"
  }
}
